    <%= link_to store_path(store), class: "text-decoration-none" do %>
      <div class="card h-100" style="border-radius: 10px;   box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19), url('<%= url_for store.photo%>');">
        <img src="https://media.istockphoto.com/id/620972806/photo/man-texting-in-the-shopping-mall.jpg?s=612x612&w=0&k=20&c=c9ByRTLwKrXzlPSBag2QzjU0pDS7P_SR07AjxqMlcU8=" class="card-img-top card-product-img" style="height: 20; width: 20;">
          <div class="card-body">
            <%= store.name %>
          </div>
      </div>
    <% end %>

